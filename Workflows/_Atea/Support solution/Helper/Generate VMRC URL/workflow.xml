<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:workflow xmlns:ns2="http://vmware.com/vco/workflow" root-name="item1" object-name="workflow:name=generic" id="d2897a1f-8161-44d0-b2a3-a07ca59b5fae" editor-version="2.0" version="0.0.0" api-version="6.0.0" restartMode="1" resumeFromFailedMode="0">
    <display-name>Generate VMRC URL</display-name>
    <position y="50.0" x="100.0"/>
    <input>
        <param name="vm" type="VC:VirtualMachine"/>
    </input>
    <output>
        <param name="url" type="string"/>
    </output>
    <workflow-item name="item0" type="end" end-mode="0" comparator="0">
        <in-binding/>
        <out-binding/>
        <position y="50.0" x="300.0"/>
    </workflow-item>
    <workflow-item name="item1" out-name="item0" type="task" comparator="0">
        <display-name>Create VMRC URL</display-name>
        <script encoded="false">var mks = vm.acquireMksTicket();&#13;
var url = "vmrc://"+mks.host+":"+mks.port+"/?mksticket="+mks.ticket+"&amp;thumbprint="+mks.sslThumbprint+"&amp;path="+mks.cfgFile&#13;
</script>
        <in-binding>
            <bind name="vm" type="VC:VirtualMachine" export-name="vm"/>
        </in-binding>
        <out-binding>
            <bind name="url" type="string" export-name="url"/>
        </out-binding>
        <description>Simple task with custom script capability.</description>
        <position y="60.0" x="170.0"/>
    </workflow-item>
    <presentation/>
</ns2:workflow>
